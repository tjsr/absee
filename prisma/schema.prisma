// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id String @id
  email String @unique
}

model Comparison {
  id String @id
  userId String
  requestTime String
  a String
  b String
}

model ComparisonResponse {
  id String @id
  selectedResponse String
}
