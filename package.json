{
  "name": "absee",
  "version": "0.0.1",
  "description": "A comparison and data store tool",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "start:frontend": "react-scripts start",
    "start:dev": "nodemon index.ts && react-scripts start",
    "build": "react-scripts build",
    "test": "ts-jest",
    "db:init": "npx prisma migrate dev --name init",
    "db:upgrade": "npx prisma migrate dev",
    "docker:stop": "docker stop restsqlite",
    "docker:sqlite": "docker run -v h/dev/absee/prisma:/db --name sqlite3 --rm -it keinos/sqlite3 sqlite3 /db/dev.db",
    "docker:restsqlite": "docker run -v h/dev/absee/prisma:/database --env DATABASE=dev.db --name restsqlite -p 8085:8085 --rm -d -t tjsrowe/restsqlite",
    "dev:upgrade": "npm run docker:stop && npm run db:upgrade && npm run docker:restsqlite"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/tjsrowe/absee.git"
  },
  "author": "Tim Rowe <tim@tjsr.id.au>",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/tjsrowe/absee/issues"
  },
  "homepage": "./",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@jest/globals": "^29.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/jest": "^29.4.1",
    "@types/node": "^18.15.1",
    "@types/request-ip": "^0.0.37",
    "@types/typescript": "^2.0.0",
    "@types/uuid": "^9.0.1",
    "cors": "^2.8.5",
    "jest": "^29.5.0",
    "prisma": "^4.11.0",
    "react-scripts": "^5.0.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "axios": "^1.3.4",
    "date-to-iso-8601-string": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-ip": "^1.0.4",
    "express-session": "^1.17.3",
    "iso8601-convert": "^1.0.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "request-ip": "^3.3.0",
    "session": "^0.1.0",
    "snowflake-uuid": "^1.0.0",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
