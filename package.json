{
  "name": "absee",
  "version": "0.0.1",
  "description": "A comparison and data store tool",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "copyswipe": "shx cp -r ../abswipe/src/components/* ./src/ui/components/",
    "clean": "rimraf dist build node_modules",
    "start:server": "tsx watch server.ts",
    "start:ui": "vite dev",
    "start": "echo Run \"npm run start:ui\" then \"npm run start:server\"",
    "start:prod": "node index.js",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider vite build && tsc",
    "test": "npx jest --runInBand --detectOpenHandles",
    "db:init:dev": "npx prisma migrate dev --name init",
    "db:upgrade": "npx prisma migrate dev",
    "db:upgrade:deploy": "npx prisma migrate deploy",
    "docker:build:deploy": "npm run docker:build:dbmigrate && npm run docker:build:app && npm run docker:tagaws",
    "docker:build": "docker build -t absee -f Dockerfile .",
    "docker:build:dbmigrate": "docker build -t absee-dbmigrate -f Dockerfile.dbmigrate .",
    "docker:tagaws": "docker tag absee-dbmigrate:latest public.ecr.aws/tjsrowe/absee:dbmigrate && docker tag absee:latest public.ecr.aws/tjsrowe/absee:latest",
    "docker:run:test": "docker run --name absee --rm -p 8281:8280 --env-file=.env.test -it absee",
    "docker:run:test:sh": "docker run --name absee --rm -p 8280:8280 --env-file=.env.test -it absee /bin/sh -",
    "docker:run:dbmigrate": "docker run --rm --env-file=.env --name absee-dbmigrate -it absee-dbmigrate",
    "docker:stop": "docker stop restsqlite",
    "docker:sqlite": "docker run -v h/dev/absee/prisma:/db --name sqlite3 --rm -it keinos/sqlite3 sqlite3 /db/dev.db",
    "docker:restsqlite": "docker run -v h/dev/absee/prisma:/database --env DATABASE=dev.db --name restsqlite -p 8085:8085 --rm -d -t tjsrowe/restsqlite",
    "dev:upgrade": "npm run docker:stop && npm run db:upgrade && npm run docker:restsqlite",
    "lint": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier --single-quote --write \"./src/**/*.{ts,tsx}\"",
    "format": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/tjsrowe/absee.git"
  },
  "author": "Tim Rowe <tim@tjsr.id.au>",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/tjsrowe/absee/issues"
  },
  "homepage": "./",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-mysql-session": "^3.0.4",
    "@types/express-session": "^1.18.0",
    "@types/is-uuid": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.6",
    "@types/morgan": "^1.9.9",
