{
  "name": "absee",
  "version": "0.0.1",
  "description": "A comparison and data store tool",
  "main": "index.ts",
  "scripts": {
    "clean": "rimraf dist build node_modules",
    "start": "npm run build && ts-node index.ts",
    "start:frontend": "cross-env GENERATE_SOURCEMAP=true react-scripts start",
    "start:dev": "nodemon index.ts && react-scripts start",
    "start:prod": "node index.js",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts build && tsc",
    "test": "ts-jest",
    "db:init:dev": "npx prisma migrate dev --name init",
    "db:upgrade": "npx prisma migrate dev",
    "db:upgrade:deploy": "npx prisma migrate deploy",
    "docker:build:deploy": "npm run docker:build:dbmigrate && npm run docker:build:app && npm run docker:tagaws",
    "docker:build": "docker build -t absee -f Dockerfile .",
    "docker:build:dbmigrate": "docker build -t absee-dbmigrate -f Dockerfile.dbmigrate .",
    "docker:tagaws": "docker tag absee-dbmigrate:latest public.ecr.aws/tjsrowe/absee:dbmigrate && docker tag absee:latest public.ecr.aws/tjsrowe/absee:latest",
    "docker:run:test": "docker run --name absee --rm -p 8280:8280 --env-file=.env.test -it absee",
    "docker:run:test:sh": "docker run --name absee --rm -p 8280:8280 --env-file=.env.test -it absee /bin/sh -",
    "docker:run:dbmigrate": "docker run --rm --env-file=.env --name absee-dbmigrate -it absee-dbmigrate",
    "docker:stop": "docker stop restsqlite",
    "docker:sqlite": "docker run -v h/dev/absee/prisma:/db --name sqlite3 --rm -it keinos/sqlite3 sqlite3 /db/dev.db",
    "docker:restsqlite": "docker run -v h/dev/absee/prisma:/database --env DATABASE=dev.db --name restsqlite -p 8085:8085 --rm -d -t tjsrowe/restsqlite",
    "dev:upgrade": "npm run docker:stop && npm run db:upgrade && npm run docker:restsqlite",
    "lint": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier --single-quote --write \"./src/**/*.{ts,tsx}\"",
    "format": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/tjsrowe/absee.git"
  },
  "author": "Tim Rowe <tim@tjsr.id.au>",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/tjsrowe/absee/issues"
  },
  "homepage": "./",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-mysql-session": "^2.1.3",
    "@types/express-session": "^1.17.7",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^29.5.0",
    "@types/js-cookie": "^3.0.3",
    "@types/morgan": "^1.9.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.3.1",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/request-ip": "^0.0.37",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "cross-env": "^7.0.3",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "prisma": "^4.12.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@react-spring/web": "^9.7.1",
    "@use-gesture/react": "^10.2.25",
    "axios": "^1.3.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-to-iso-8601-string": "^2.0.0",
    "dotenv": "^16.0.3",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "express-ip": "^1.0.4",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.3",
    "is-uuid": "^1.0.2",
    "iso8601-convert": "^1.0.0",
    "js-cookie": "^3.0.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "request-ip": "^3.3.0",
    "session": "^0.1.0",
    "snowflake-uuid": "^1.0.0",
    "styled-components": "^5.3.9",
    "superjson": "^1.12.2",
    "title-case": "^3.0.3",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
